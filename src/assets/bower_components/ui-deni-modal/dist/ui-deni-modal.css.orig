.inputfile + label {
  font-size: 12px;
  font-weight: 700;
  cursor: pointer;
  display: inline-block;
  padding: 5px 10px;
  margin-left: 4px; }

.inputfile + label svg {
  width: 18px;
  height: 18px;
  vertical-align: middle;
  fill: currentColor;
  margin-top: -4px;
  margin-right: 4px; }

.inputfile-2 + label {
  color: #0099e6;
  border: 1px solid currentColor; }

.inputfile-2 + label:hover {
  color: #33bbff; }

